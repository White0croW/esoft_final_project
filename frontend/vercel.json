{
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/api"
        },
        {
            "src": "/static/.*",
            "headers": {
                "Cache-Control": "s-maxage=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/",
            "status": 200
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}